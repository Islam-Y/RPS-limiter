      "id": 1,
      "type": "timeseries",
      "title": "AI Requests by Result",
      "datasource": { "type": "prometheus", "uid": "prometheus-main" },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
      "targets": [
        {
          "expr": "sum(rate(ai_limit_config_requests_total{job=\"ai-module\"}[$__rate_interval])) by (result)",
          "legendFormat": "{{result}}",
          "refId": "A"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "reqps", "decimals": 3 }, "overrides": [] },
      "options": {
        "legend": { "displayMode": "table", "placement": "bottom", "showLegend": true },
